citHeader("To cite mikRopML in publications use:")

citEntry(
  entry = "manual",
  title = "{mikRopML}: User-Friendly Machine Learning Package for Classification Problems",
  author = personList(as.person("Begüm Topçuoğlu"),
                      as.person("Zena Lapp"),
                      as.person("Kelly L. Sovacool"),
                      as.person("Evan Snitkin"),
                      as.person("Jenna Wiens"),
                      as.person("Patrick D. Schloss")),
  year = "2020",
  url = "http://www.schlosslab.org/mikRopML",
  textVersion = paste("Topçuoğlu et al. (2020).",
                      "mikRopML: User-Friendly Machine Learning Package for Classification Problems.",
                      "http://www.schlosslab.org/mikRopML")
)
